{
  "extends": "tslint-config-airbnb",
  "rules": {
    "ter-indent": [true, 2],
    "semicolon": [true, "never"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
    "trailing-comma": [true, "never"],
    "max-line-length": [true, 160],
    "ter-arrow-parens": [false]
  }
}
